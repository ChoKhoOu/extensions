{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "coda-docs-search",
  "title": "Coda Docs Search",
  "description": "",
  "icon": "coda-logo-512.png",
  "author": "ChoKhoOu",
  "categories": [
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Docs Search",
      "subtitle": "Coda Docs Search",
      "description": "Coda Docs Search",
      "mode": "view",
      "preferences": [
        {
          "name": "apiToken",
          "description": "Coda API token generated from your account settings.",
          "type": "password",
          "required": true,
          "title": "API Token"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "axios": "^1.2.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
